{
    "name": "orchestra/model",
    "description": "Model Component for Orchestra Platform",
    "homepage": "http://orchestraplatform.com/docs/latest/components/model",
    "keywords": ["orchestra-platform", "orchestral", "model", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Model\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Model\\Tests\\" : "tests/"
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "laravie/dhosa": "^1.1",
        "orchestra/contracts": "^6.0",
        "orchestra/database": "dev-master",
        "orchestra/support": "dev-master"
    },
    "require-dev": {
        "orchestra/auth": "^6.0",
        "orchestra/memory": "^6.0",
        "orchestra/testbench": "^6.17"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
